@model IEnumerable<RoundpayFinTech.AppCode.Model.Level>

@if (Model != null && Model.Any())
{
    int i = 0;
    foreach (var item in Model)
    {
        i++;
        <tr data-parent-id="@item.ParentId">
            <td>@i</td>
            <td>@item.LevelCount</td>
            <td>@item.TotalCountAT</td>
        </tr>
    }
}