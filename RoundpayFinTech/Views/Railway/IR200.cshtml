@model RoundpayFinTech.AppCode.Model.IRReturnHitModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Roundpay - IRCTC</title>
    <link href="~/css/style_IR.css" rel="stylesheet" />
    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/lib/bootstrap/dist/fonts/css/fontawesome-all.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/lib/bootstrap/dist/fonts/css/fontawesome-all.css" />
    </environment>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
</head>
<body>
    <div class="container loader">
        <div class="row">
            <div class="col-md-12">
                <img src="@Model.ErrorCode" class="sec403" />
                @if (Model.Msg != null)
                {
                    @*<h3 class="message-sec">@Model.Msg</h3>*@
                    <h5 class="message-sec">@Model.Msg! Your page will be redirected back to IRCTC. Do not press back or cancel button.</h5>
                }
            </div>
        </div>
    </div>
    <div style="display:none">
        <form method="post" action="@Model.RU">
            <input type="text" name="encdata" value="@Model.EncResp" />
            <input type="submit" id="btnSubmit" />
        </form>
    </div>
    <script>
        $(document).ready(function () {
            let isDv = '@Model.IsDV';
            console.log(isDv);
            debugger;
            if (isDv == 'True') {
                $('#btnSubmit').click();
            }
            else {
                window.setTimeout(function () {
                    $('#btnSubmit').click();
                }, 5000);
            }
        });
    </script>
</body>
</html>

