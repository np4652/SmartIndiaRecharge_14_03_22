@model IEnumerable<RoundpayFinTech.AppCode.Model.UserCallBackModel>

@{ var counter = 1; }

@if (Model.Any())
{
    foreach (var item in Model)
    {

        <tr>
            <td>@counter</td>
            <td>
                <div class="btn-group dropup">
                    <a href="#" id="act_@counter" class="btn btn-outline-dark btn-sm btn-block dropdown-toggle" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expaned="false"></a>
                    <div class="dropdown-menu" aria-labelledby="act_@counter">
                        <h6 class="dropdown-header">Action Menu</h6>
                        <a href="#" class="dropdown-item ddledit" id="edt_@item.CallbackType">Edit</a>
                    </div>
                </div>
            </td>
            <td>@item.CallBackName</td>
            <td>@item.URL</td>
            <td>@item.UpdateUrl</td>
            <td>@item.Remark</td>
        </tr>
        counter = counter + 1;
    }
}
else
{
    <tr rowspan="2">
        <td colspan="8">
            <center>No Record Found</center>
        </td>
    </tr>
}

