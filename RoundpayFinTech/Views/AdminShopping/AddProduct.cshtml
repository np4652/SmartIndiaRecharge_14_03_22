@using RoundpayFinTech.AppCode.Configuration
@model RoundpayFinTech.AppCode.Model.Shopping.ProductModel
@{
    ViewData["Title"] = "Add Product";
    var cList = new SelectList(Model.MainCategory, "MainCategoryID", "Name");
    var sList1 = Model.CategoryList != null ? new SelectList(Model.CategoryList, "SubCategoryId", "Name") : null;
    var sList2 = Model.SubCategory != null ? new SelectList(Model.SubCategory, "SubCategoryId", "Name") : null;
    var MasterProduct = Model.MasterProduct != null ? new SelectList(Model.MasterProduct, "ProductID", "ProductName") : null;
    var Brands = Model.Brands != null ? new SelectList(Model.Brands, "BrandId", "BrandName") : null;
    var Vendors = new SelectList(Model.Vendors, "vendorsID", "vendorsName");
    int Role = Model.Role;
}

<link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-beta.1/dist/css/select2.min.css" rel="stylesheet" />
<link href="~/css/Shopping.css" rel="stylesheet" />
<style>
    .cus-control {
        display: inherit !important;
        width: inherit !important;
        position: relative;
        top: 3px;
    }

</style>
<div aria-label="breadcrumb" class="breadcrumb" id="page-header">
    <div class="container-fluid">
        <h5 class="float-left"><i class="fa fa-users"></i> @ViewData["Title"]</h5>
        <ol class="breadcrumb float-right">
            <li class="breadcrumb-item"><a href="javascript:void(0)">Home</a></li>
            <li class="breadcrumb-item">Shopping</li>
            <li class="breadcrumb-item active">@ViewData["Title"]</li>
        </ol>
    </div>
</div>
<div id="__p" class="main-temp-body">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card mt-2">
                    <div class="card-header bg-gradientorange text-white line-height-34">
                        <div class="row">
                            <div class="col-sm-10">
                                <i class="fas fa-link"></i> @ViewData["Title"]
                            </div>
                            <div class="col-sm-2 text-right">
                                <div class="btn-group btn-group-sm">
                                    <button class="btn btn-sm btn-outline-light pl-2 pr-2 font-bold" data-Item-id="0" id="btnNew"><i class="fa fa-plus"></i> NEW</button>
                                    <input type="hidden" id="hfdPDID" value="@Model.Detail.ProductDetail.ProductDetailID" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="card-body pb-0">
                        <div class="row">
                            <div class="col-sm-3">
                                <div class="form-group">
                                    <span class="text-monospace">Category<sup class="text-danger">*</sup></span>
                                    <select class="form-control" id="ddlCategory" asp-items="@cList" asp-for="@Model.Detail.ProductDetail.CategoryID" required="required" value="0" tabindex="1">
                                        <option value="0">:: Choose Category ::</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div class="form-group">
                                    <span class="text-monospace">Level-1<sup class="text-danger">*</sup></span>
                                    <select class="form-control" id="ddlLevel1" asp-items="@sList1" asp-for="@Model.Detail.ProductDetail.SubCategoryID1" required="required" value="0" tabindex="2">
                                        <option>:: Choose Level 1 ::</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div class="form-group">
                                    <span class="text-monospace">Level-2<sup class="text-danger">*</sup></span>
                                    <select class="form-control" id="ddlLevel2" asp-items="@sList2" asp-for="@Model.Detail.ProductDetail.SubCategoryID2" required="required" value="0" tabindex="3">
                                        <option>:: Choose Level 2 ::</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div class="form-group">
                                    <span class="text-monospace">Product<sup class="text-danger">*</sup></span>
                                    <select class="form-control" id="ddlProduct" asp-items="@MasterProduct" asp-for="@Model.Detail.ProductDetail.ProductID" required="required" value="0" tabindex="4">
                                        <option>:: Product ::</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div class="form-group">
                                    <span class="text-monospace">Brand<sup class="text-danger">*</sup></span>
                                    <select class="form-control" id="ddlBrand" required="required" asp-items="@Brands" asp-for="@Model.Detail.ProductDetail.BrandID" tabindex="5">
                                        <option value="0">:: Select Brand ::</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div class="form-group">
                                    <span class="text-monospace">Vendor<sup class="text-danger">*</sup></span>
                                    <select class="form-control" id="ddlVendor" required="required" asp-items="@Vendors" asp-for="@Model.Detail.ProductDetail.VendorID" tabindex="6">
