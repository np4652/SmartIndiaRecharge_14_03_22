                                <div class="form-group">
                                    <span class="text-monospace">Batch</span>
                                    <input type="text" class="form-control" id="txtBatch" placeholder="Batch" tabindex="8" value="@Model.Detail.ProductDetail.Batch" />
                                </div>
                            </div>
                            @*Div start here*@
