@using RoundpayFinTech.AppCode.Model.ProcModel
@model IEnumerable<RangeSlabDetailDisplayLvl>

@{
    var couter = 1;
}

@if (Model.Any())
{
    foreach (var item in Model)
    {
<tr class="tr_bstatus">
    <td scope="col">@couter</td>
    <td scope="col">@item.Operator<small class="form-text text-muted mt-0">@item.OpType</small></td>
    <td scope="col">@item.SPKey</td>
    <td scope="col">@item.IsBilling</td>
    <td scope="col">@item.BusinessModel</td>
    <td class="text-center">
        <div class="row">
            @foreach (var roleComm in item.RoleCommission)
            {
                <div class="col-md-3 text-nowrap">
                    <span class="text-monospace"> @(roleComm.Prefix + "[" + (roleComm.CommType == 1 ? "SUR " : "COM ") + roleComm.Comm + (roleComm.AmtType == 0 ? "%" : "") + "]")</span>
                </div>
            }

        </div>
    </td>
    <td class="text-center">
        <div class="row">
            @foreach (var roleComm in item.RoleCommission)
            {
                <div class="col-md-3 text-nowrap">
                    <span class="text-monospace">@roleComm.MaxComm</span>
                </div>
            }
        </div>
    </td>
    <td scope="col">@item.MinRange-@item.MaxRange</td>
    <td scope="col">@item.Min.ToString() - @item.Max.ToString()</td>
</tr>
        couter = couter + 1;
    }
}
else
{
    <tr>
        <td colspan="8"></td>

    </tr>
    <tr>
        <td colspan="8">
            <center>No Record Found</center>
        </td>
    </tr>
}

