@model List<RoundpayFinTech.AppCode.Model.ASAreaMaster>

@{
    var counter = 1;
}

@if (Model.Any())
{
    foreach (var item in Model)
    {
        <tr data-item-id="@item.AreaID" data-ar="@item.Area">
            <td>@counter</td>
            <td>
                <div class="btn-group dropup">
                    <a href="#" id="act_@counter" class="btn btn-outline-dark btn-sm btn-block dropdown-toggle" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expaned="false"></a>
                    <div class="dropdown-menu" aria-labelledby="act_@counter">
                        <h6 class="dropdown-header">Action Menu</h6>
                        <a href="#" class="dropdown-item ddlRemove" id="rem_@item.AreaID">Edit</a>
                    </div>
                </div>
            </td>
            <td>@item.Area</td>
            <td>@item.EntryDate</td>
            <td>@item.ModifyDate</td>
        </tr>
        counter = counter + 1;
    }
}
else
{
    <tr rowspan="2">
        <td colspan="9">
            <center>No Record Found</center>
        </td>
    </tr>
}

