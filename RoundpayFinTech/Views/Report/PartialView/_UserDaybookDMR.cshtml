@model List<RoundpayFinTech.AppCode.Model.DMRDaybook>

@{
    var couter = 1;
    var IsEndUser = ViewBag.IsEndUser==null?false:(bool)ViewBag.IsEndUser;
}

@if (Model.Count > 0)
{
  

    foreach (var item in Model)
    {
<tr>
    <th class="text-right">@couter</th>
    <td class="text-right">@item.TotalHits</td>
    <td class="text-right">@item.TotalAmount</td>
    <td class="text-right">@item.CCF</td>
    <td class="text-right">@item.BaseAmount</td>
    <td class="text-right">@item.Surcharge</td>
    <td class="text-right">@item.GSTOnSurcharge</td>
    <td class="text-right">@item.AmountAfterSurcharge</td>
    <td class="text-right">@item.RefundGST</td>
    <td class="text-right">@item.AmountWithTDS</td>
    <td class="text-right">@item.TDS</td>
    <td class="text-right">@item.CreditedAmount</td>
    @if (!IsEndUser)
    {
        <td class="text-right">@item.SelfCommission</td>
    }
    @if (!IsEndUser)
    {
        <td class="text-right">@item.TeamCommission</td>
    }
</tr>

        couter = couter + 1;

    }

}
else
{
    <tr rowspan="2">
        <td colspan="14">
            <center>No Record Found</center>
        </td>
    </tr>
}


