@model IEnumerable<RoundpayFinTech.AppCode.Model.WalletRequest>

@{ var couter = 1; }

@if (Model.Any())
{
    foreach (var item in Model)
    {
        <tr data-item-id="@item.ID">
            <td>@couter</td>
            <td>
                <div class="btn-group dropup">
                    <input type="checkbox" id="@item.ID" name="childcheck">
                </div>
            </td>
            <td>
                <input type="text" id="txtBankRRN_@item.ID" name="txtBankRRN" class="form-control" maxlength="12" />
                @if (ApplicationSetting.IsRealSettlement)
                {
                    <span class="btn btn-sm btn-outline-info" id="spnReal_@(item.ID)">Real Settlement</span>
                }
            </td>
            <td>@item.UserName <small class="font-weight-bold text-monospace"> [@item.Mobile]</small></td>
            <td>@item.BankName</td>
            <td>@item.AccountHolder  <small class="font-weight-bold text-monospace"> [@item.AccountNumber]</small></td>
            <td>@item.TransMode</td>
            <td class="text-right">@item.Amount</td>
            <td>@item.Charge</td>
            <td>@item.MiniBankBalance</td>
            <td>@item.MiniBankCapping</td>
            <td>@item.EntryDate</td>

        </tr>
        couter = couter + 1;
    }
}
else
{
    <tr>
        <td colspan="12" rowspan="2" class="text-center">
            No Record Found
        </td>
    </tr>
}
