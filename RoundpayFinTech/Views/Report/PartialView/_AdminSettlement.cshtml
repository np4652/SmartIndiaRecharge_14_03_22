@using RoundpayFinTech.AppCode.Model.ProcModel
@model IEnumerable<AdminSettlement>
@{
    var c = 1;
}
@if (Model.Any())
{
    foreach (var item in Model)
    {
<tr>
    <th>@c</th>
    <td>@item._TransactionDate</td>
    <td class="text-right">@item._Opening</td>
    <td class="text-right">@item._FundTransfered</td>
    <td class="text-right">@item._Refund</td>
    <td class="text-right">@item._Commission</td>
    <td class="text-right">@item._CCFComm</td>
    <td class="text-right">@item._FundDeducted</td>
    <td class="text-right">@item._Surcharge</td>
    <td class="text-right">@item._SuccessPrepaid</td>
    <td class="text-right">@item._SuccessPostpaid</td>
    <td class="text-right">@item._SuccessDTH</td>
    <td class="text-right">@item._SuccessUtility</td>
    <td class="text-right">@item._SuccessDMT</td>
    <td class="text-right">@item._SuccessDMTCharge</td>
    <td class="text-right">@item._TotalSUccess</td>
    <td class="text-right">@item._OtherCharge</td>
    <td class="text-right">@item._CCFCommDebited</td>
    <td class="text-right">@item._Closing</td>
    <td class="text-right">@item._Expected</td>
</tr>
        c = c + 1;
    }

}
else {
<tr rowspan="2">
    <td class="text-center" colspan="20">No Record Found !</td>
</tr>
}
