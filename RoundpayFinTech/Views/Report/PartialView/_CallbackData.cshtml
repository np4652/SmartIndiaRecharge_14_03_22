@model IEnumerable<CallbackRequests>;
@{
    var i = 1;
}
@if (Model.Any())
{
    @foreach (var item in Model)
    {
<tr>
    <th>@i</th>
    <td>@item.EntryDate</td>
    <td><textarea class="form-control bg-transparent border-0" cols="70" readonly="readonly">@item.Content</textarea></td>
    <td>@item.Path</td>
    <td>@item.Method</td>
    <td>@item.RequestIP</td>
</tr>
        i = i + 1;
    }
}
else
{
<tr>
    <td colspan="4" rowspan="2" class="text-center">No record found!</td>
</tr>
}
