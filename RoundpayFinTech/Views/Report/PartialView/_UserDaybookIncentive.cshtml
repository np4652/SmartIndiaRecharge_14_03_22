@model IEnumerable<RoundpayFinTech.AppCode.Model.Daybook>

@{
    var couter = 1;
}
    <div class="col-md-12">
        <button type="button" class="close" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
        <h5>Operatorwise Incentive</h5>
        <table class="table table-hover table-responsive-sm mb-0">
            <thead class="shadow">
                <tr>
                    <th class="text-right">#</th>
                    <th>Operator</th>
                    <th class="text-right">Denomination</th>
                    <th class="text-right">Total Hits</th>
                    <th class="text-right" scope="col">Incentive</th>
                </tr>
            </thead>
            <tbody>
                @if (Model.Any())
                {
                    foreach (var item in Model)
                    {
                        <tr>
                            <td class="text-right">@couter</td>
                            <td>@item.Operator</td>
                            <td class="text-right">@item.Denomination</td>
                            <td class="text-right">@item.TotalHits</td>
                            <td class="text-right">@item.Incentive</td>
                        </tr>

                        couter = couter + 1;

                    }
                }
                else
                {
                    <tr rowspan="2">
                        <td colspan="5">
                            <center>No Record Found</center>
                        </td>
                    </tr>
                }


            </tbody>
        </table>
        <button class="btn btn-dark" id="mdlCancel">Cancel</button>
    </div>

