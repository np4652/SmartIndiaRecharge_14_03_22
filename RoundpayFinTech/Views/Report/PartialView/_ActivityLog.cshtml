@model List<Fintech.AppCode.Model.Reports.UserActivityLog>
@{
    var conuter = 1;
}
@if (Model.Any())
{
    foreach (var item in Model)
    {
        <tr>
            <th>@conuter</th>
            <td>@item.Name <small class="font-weight-bold text-monospace"> [@item.MobileNo]</small></td>
            <td>@item.Action </td>
            <td>@item.EntryDate</td>
        </tr>
        conuter = conuter + 1;
    }
}
else
{
    <tr>
        <td colspan="5"></td>

    </tr>
    <tr>
        <td colspan="5">
            <center>No Record Found</center>
        </td>
    </tr>
}