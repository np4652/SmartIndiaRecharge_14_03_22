                        <td class="bg-secondary text-white text-right">@item.Pri10</td>
                        <td class="bg-info text-white text-right">@item.Sec10</td>
                        <td class="bg-success text-white text-right">@item.Ter10</td>
                    </tr>
                    counter++;
                    pri1 += @item.Pri1; pri2 += item.Pri2; pri3 += item.Pri3; pri4 += item.Pri4; pri5 += item.Pri5;
                    pri6 += item.Pri6; pri7 += item.Pri7; pri8 += item.Pri8; pri9 += item.Pri9; pri10 += item.Pri10;
                    sec1 += @item.Sec1; sec2 += item.Sec2; sec3 += item.Sec3; sec4 += item.Sec4; sec5 += item.Sec5;
                    sec6 += item.Sec6; sec7 += item.Sec7; sec8 += item.Sec8; sec9 += item.Sec9; sec10 += item.Sec10;
