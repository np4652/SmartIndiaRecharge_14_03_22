                <th class="text-center bg-secondary ">
                    Primary
                </th>
                <th class="text-center bg-info ">
                    Secoundary
                </th>
                <th class="text-center bg-success  ">
                    Tertiary
                </th>
                <th class="text-center bg-secondary ">
                    Primary
                </th>
                <th class="text-center bg-info ">
                    Secoundary
                </th>
                <th class="text-center  bg-success  ">
                    Tertiary
                </th>
                <th class="text-center bg-secondary ">
                    Primary
                </th>
                <th class="text-center bg-info ">
                    Secoundary
                </th>
                <th class="text-center bg-success  ">
                    Tertiary
                </th>
                <th class="text-center bg-secondary ">
                    Primary
