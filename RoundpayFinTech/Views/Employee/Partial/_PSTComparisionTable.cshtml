@model IEnumerable<RoundpayFinTech.AppCode.Model.Employee.PSTComparisionTable>
@{
    decimal pri1 = 0, pri2 = 0, pri3 = 0, pri4 = 0, pri5 = 0, pri6 = 0, pri7 = 0, pri8 = 0, pri9 = 0, pri10 = 0;
    decimal sec1 = 0, sec2 = 0, sec3 = 0, sec4 = 0, sec5 = 0, sec6 = 0, sec7 = 0, sec8 = 0, sec9 = 0, sec10 = 0;
    decimal ter1 = 0, ter2 = 0, ter3 = 0, ter4 = 0, ter5 = 0, ter6 = 0, ter7 = 0, ter8 = 0, ter9 = 0, ter10 = 0;
}
<style>
    .bg-secondary {
        background-color: #6665bd !important;
    }

    .bg-info {
        background-color: #7574d2 !important;
    }

    .bg-success {
        background-color: #9695f2 !important;
    }
</style>
@if (Model.Any())
{
    <table class="table table-bordered">
        <thead class="bg-tableth">
            <tr>
                <th rowspan="2">Sr</th>
                <th rowspan="2">UserName</th>
                <th colspan="3" class="text-center">@DateTime.Now.AddDays(-1).ToString("dd MMM yy")</th>
                <th colspan="3" class="text-center">@DateTime.Now.AddDays(-2).ToString("dd MMM yy")</th>
                <th colspan="3" class="text-center">@DateTime.Now.AddDays(-3).ToString("dd MMM yy")</th>
                <th colspan="3" class="text-center">@DateTime.Now.AddDays(-4).ToString("dd MMM yy")</th>
                <th colspan="3" class="text-center">@DateTime.Now.AddDays(-5).ToString("dd MMM yy")</th>
                <th colspan="3" class="text-center">@DateTime.Now.AddDays(-6).ToString("dd MMM yy")</th>
                <th colspan="3" class="text-center">@DateTime.Now.AddDays(-7).ToString("dd MMM yy")</th>
                <th colspan="3" class="text-center">@DateTime.Now.AddDays(-8).ToString("dd MMM yy")</th>
                <th colspan="3" class="text-center">@DateTime.Now.AddDays(-9).ToString("dd MMM yy")</th>
                <th colspan="3" class="text-center">@DateTime.Now.AddDays(-10).ToString("dd MMM yy")</th>
            </tr>
            <tr>
                <th class="text-center bg-secondary">
