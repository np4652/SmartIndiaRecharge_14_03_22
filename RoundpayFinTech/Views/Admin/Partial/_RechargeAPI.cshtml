                        @Html.TextBox("txtBalanceKey", _Model.BalanceKey, new { @class = "form-control", @placeholder = "Enter BalanceKey Value", @aria_describedby = "txtBalanceKeyHelp", @tabindex = 15 })
                        <small id="txtBalanceKeyHelp" class="form-text text-muted d-none"></small>
                    </div>
                    <div class="col-md form-group">
                        @Html.Label("ddlMethodBill", "Bill Method Type", new { @class = "lbl" })
                        <select id="ddlMethodBill" aria-describedby="billmethodHelp" tabindex="16" class="custom-select custom-select-md mb3" asp-for="@_Model.BillReqMethod">
                            <option value="GET">GET</option>
                            <option value="POST">POST</option>
                        </select>
                        <small id="billmethodHelp" class="form-text text-muted d-none">Bill Method is mandatory</small>
                    </div>
                    <div class="col-md form-group">
                        @Html.Label("ddlResTypeBill", "Response Type", new { @class = "lbl" })

                        <select id="ddlResTypeBill" aria-describedby="resHelp" tabindex="17" class="custom-select custom-select-md mb3" asp-for="@_Model.BillResTypeID">
                            <option value="1">JSON</option>
                            <option value="2">XML</option>
