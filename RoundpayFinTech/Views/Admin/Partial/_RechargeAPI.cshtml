    <div class="col-9">
        <h3 class="mb-0 mt-0 bg-white rounded "> API Form</h3>
        <hr />
        <div class="scroll-content">
            <div class="container">
                <div class="form-group">
                    @Html.Label("txtName", "Name", new { @class = "lbl" })
                    <div class="input-group">
                        @Html.TextBox("txtName", _Model.Name, new { @class = "form-control text-success", @placeholder = "Enter Name", @aria_describedby = "nameHelp", @tabindex = 1 })
                        <div class="input-group-append">
                            <span class="input-group-text switch switch-sm bg-white align" style="padding:0;">
                                <select class="form-control-sm" id="ddlApiType" tabindex="2" style="border:0;" asp-for="@_Model.APIType">
                                    <option value="1">General</option>
                                    <option value="2">Lapu</option>
                                    <option value="3">Real</option>
                                    <option value="4">Manual</option>
                                </select>
                            </span>
                        </div>
                    </div>
                    <small id="nameHelp" class="form-text text-muted d-none">Name is mandatory</small>
                </div>
                <div class="form-group">
                    @Html.Label("txtURL", "API URL", new { @class = "lbl" })
                    @Html.TextArea("txtURL", _Model.URL, new { @class = "form-control text-danger", @placeholder = "Enter API URL", @aria_describedby = "urlHelp", @tabindex = 3 })
                    <small id="urlHelp" class="form-text text-muted d-none">URL is mandatory</small>
                </div>
                <div class="form-group">
                    @Html.Label("txtStsChkURL", "Status Check URL", new { @class = "lbl" })
                    <select style="float:right;" class="text-danger" id="ddldateFormat" asp-for="@_Model.DFormatID">
                        <option value="0">:: Date Format(If Required) ::</option>
                        <option value="1">yyyy-MM-dd</option>
                        <option value="2">yyyy-MMM-dd</option>
                        <option value="3">yy-MM-dd</option>
                        <option value="4">dd-MMM-yyyy</option>
                        <option value="5">dd-MMM-yy</option>
                        <option value="6">dd MMM yyyy</option>
                        <option value="7">dd MMM yy</option>
                    </select>
                    @Html.TextArea("txtStsChkURL", _Model.StatusCheckURL, new { @class = "form-control", @placeholder = "Enter Status Check URL", @aria_describedby = "stsURLHelp", @tabindex = 4 })
                    <small id="stsURLHelp" class="form-text text-muted d-none">Status Check URL is not mandatory</small>
                </div>
                <div class="form-group">
                    @Html.Label("txtBalURL", "Balance URL", new { @class = "lbl" })
                    @Html.TextArea("txtBalURL", _Model.BalanceURL, new { @class = "form-control", @placeholder = "Enter Balance URL", @aria_describedby = "balURLHelp", @tabindex = 5 })
                    <small id="balURLHelp" class="form-text text-muted d-none"></small>
                </div>
                <div class="form-group">
                    @Html.Label("txtDisURL", "Dispute URL", new { @class = "lbl" })
                    @Html.TextArea("txtDisURL", _Model.DisputeURL, new { @class = "form-control", @placeholder = "Enter Dispute URL", @aria_describedby = "disURLHelp", @tabindex = 6 })
                    <small id="disURLHelp" class="form-text text-muted d-none"></small>
                </div>
                <div class="form-group">
                    @Html.Label("txtFetchBill", "Fetch Bill URL", new { @class = "lbl" })
                    @Html.TextArea("txtFetchBill", _Model.FetchBillURL, new { @class = "form-control", @placeholder = "Enter FetchBill URL", @aria_describedby = "billFetchURLHelp", @tabindex = 7 })
                    <small id="billFetchURLHelp" class="form-text text-muted d-none"></small>
                </div>
                <div class="form-group">
                    @Html.Label("txtValidateURL", "Validate URL", new { @class = "lbl" })
                    @Html.TextArea("txtValidateURL", _Model.ValidateURL, new { @class = "form-control", @placeholder = "Enter Validate URL", @aria_describedby = "billValidateURLHelp", @tabindex = 7 })
                    <small id="billValidateURLHelp" class="form-text text-muted d-none"></small>
                </div>
                <div class="form-row">
                    <div class="col-md form-group">
                        @Html.Label("ddlGroup", "API Group", new { @class = "lbl" })
                        <select id="ddlGroup" tabindex="21" class="custom-select custom-select-md" asp-items="@lst" asp-for="@(_Model.GroupID)">
                            <option value="0" class="text-info">New Group</option>
                        </select>
                    </div>
                    <div class="col-md form-group">
                        @Html.Label("txtGroupName", "Group Name", new { @class = "lbl" })
                        @Html.TextBox("txtGroupName", _Model.GroupName, new { @class = "form-control", @placeholder = "Enter Group Name", @aria_describedby = "txtGroupNameHelp", @tabindex = 8 })
                        <small id="txtGroupNameHelp" class="form-text text-muted d-none">Group Name can not empty</small>
                    </div>
                    <div class="col-md form-group">
                        @Html.Label("txtGroupCode", "Group Code", new { @class = "lbl" })
                        @Html.TextBox("txtGroupCode", _Model.GroupCode, new { @class = "form-control", @placeholder = "Enter Group Code", @aria_describedby = "txtGroupCodeHelp", @tabindex = 8 })
                        <small id="txtGroupCodeHelp" class="form-text text-muted d-none">Group Code is mandatory</small>
                    </div>
                    <div class="col-md form-group @(ApplicationSetting.IsDMTWithPIPE?"":"d-none")">
                        @Html.Label("txtMaxLimitPerTransaction", "Enter MaxLimitPerTransaction", new { @class = "lbl" })
                        @Html.TextBox("txtMaxLimitPerTransaction", _Model.MaxLimitPerTransaction, new { @class = "form-control", @placeholder = "Enter MaxLimitPerTransaction", @aria_describedby = "MaxLimitPerTransactionH", @tabindex = 35 })
                        <small id="MaxLimitPerTransactionH" class="form-text text-muted d-none"></small>
                    </div>
                </div>
                <div class="form-row">
                    <div class="col-md form-group">
                        @Html.Label("txtBillNoKey", "BillNoKey", new { @class = "lbl" })
                        @Html.TextBox("txtBillNoKey", _Model.BillNoKey, new { @class = "form-control", @placeholder = "Enter BillNoKey Key", @aria_describedby = "BillNoKeyHelp", @tabindex = 8 })
                        <small id="BillNoKeyHelp" class="form-text text-muted d-none"></small>
                    </div>
                    <div class="col-md form-group">
                        @Html.Label("txtBillDateKey", "BillDateKey", new { @class = "lbl" })
                        @Html.TextBox("txtBillDateKey", _Model.BillDateKey, new { @class = "form-control", @placeholder = "Enter BillDateKey", @aria_describedby = "BillDateKeyHelp", @tabindex = 9 })
                        <small id="BillDateKeyHelp" class="form-text text-muted d-none"></small>
                    </div>
                    <div class="col-md form-group">
                        @Html.Label("txtDueDateKey", "DueDateKey", new { @class = "lbl" })
                        @Html.TextBox("txtDueDateKey", _Model.DueDateKey, new { @class = "form-control", @placeholder = "Enter DueDateKey", @aria_describedby = "DueDateKeyHelp", @tabindex = 10 })
                        <small id="DueDateKeyHelp" class="form-text text-muted d-none"></small>
                    </div>
                    <div class="col-md form-group">
