                        @Html.Label("txtAdditionalInfoListKey", "AdditionalInfoListKey", new { @class = "lbl" })
                        @Html.TextBox("txtAdditionalInfoListKey", _Model.AdditionalInfoListKey, new { @class = "form-control", @placeholder = "Enter AdditionalInfoList Key", @aria_describedby = "txtAdditionalInfoListKeyHelp", @tabindex = 17 })
                        <small id="txtAdditionalInfoListKeyHelp" class="form-text text-muted d-none"></small>
                    </div>
                    <div class="col-md form-group">
                        @Html.Label("txtAdditionalInfoKey", "AdditionalInfoKey", new { @class = "lbl" })
                        @Html.TextBox("txtAdditionalInfoKey", _Model.AdditionalInfoKey, new { @class = "form-control", @placeholder = "Enter AdditionalInfoKey", @aria_describedby = "txtAdditionalInfoKeyHelp", @tabindex = 17 })
                        <small id="txtAdditionalInfoKeyHelp" class="form-text text-muted d-none"></small>
                    </div>
                    <div class="col-md form-group">
                        @Html.Label("txtAdditionalInfoValue", "AdditionalInfoValue", new { @class = "lbl" })
                        @Html.TextBox("txtAdditionalInfoValue", _Model.AdditionalInfoValue, new { @class = "form-control", @placeholder = "Enter AdditionalInfoValue", @aria_describedby = "txtAdditionalInfoValueeHelp", @tabindex = 17 })
                        <small id="txtAdditionalInfoValueHelp" class="form-text text-muted d-none"></small>
                    </div>
                    <div class="col-md form-group">
                        @Html.Label("txtValidationStatusKey", "Validation Status Key", new { @class = "lbl" })
                        @Html.TextBox("txtValidationStatusKey", _Model.ValidationStatusKey, new { @class = "form-control", @placeholder = "Enter StatusName Key", @aria_describedby = "stsNameHelp", @tabindex = 18 })
                        <small id="stsNameHelp" class="form-text text-muted d-none"></small>
                    </div>
                    <div class="col-md form-group">
                        @Html.Label("txtValidationStatusValue", "Validation Status Value", new { @class = "lbl" })
                        @Html.TextBox("txtValidationStatusValue", _Model.ValidationStatusValue, new { @class = "form-control", @placeholder = "Enter Validation Status Value", @aria_describedby = "stsNameHelp", @tabindex = 18 })
                        <small id="stsNameHelp" class="form-text text-muted d-none"></small>
                    </div>
                </div>
                <div class="form-row">
                    <div class="col-md form-group">
                        @Html.Label("txtStatusName", "Status Key", new { @class = "lbl" })
                        @Html.TextBox("txtStatusName", _Model.StatusName, new { @class = "form-control", @placeholder = "Enter StatusName Key", @aria_describedby = "stsNameHelp", @tabindex = 18 })
                        <small id="stsNameHelp" class="form-text text-muted d-none"></small>
                    </div>
                    <div class="col-md form-group">
                        @Html.Label("txtLiveID", "Live ID Key", new { @class = "lbl" })
                        @Html.TextBox("txtLiveID", _Model.LiveID, new { @class = "form-control", @placeholder = "Enter Live ID Key", @aria_describedby = "liveHelp", @tabindex = 19 })
                        <small id="liveHelp" class="form-text text-muted d-none"></small>
                    </div>
                    <div class="col-md form-group">
                        @Html.Label("txtVendorID", "Vendor ID Key", new { @class = "lbl" })
                        @Html.TextBox("txtVendorID", _Model.VendorID, new { @class = "form-control", @placeholder = "Enter Vendor ID Key", @aria_describedby = "vendorHelp", @tabindex = 20 })
                        <small id="vendorHelp" class="form-text text-muted d-none"></small>
                    </div>
                    <div class="col-md form-group">
                        @Html.Label("txtErrKey", "ErrorCode Key", new { @class = "lbl" })
                        @Html.TextBox("txtErrKey", _Model.ErrorCodeKey, new { @class = "form-control", @placeholder = "Enter ErrorCode Key", @aria_describedby = "errCodeHelp", @tabindex = 21 })
                        <small id="errCodeHelp" class="form-text text-muted d-none"></small>
                    </div>
                    <div class="col-md form-group">
                        @Html.Label("txtMsgKey", "Message Key", new { @class = "lbl" })
                        @Html.TextBox("txtMsgKey", _Model.MsgKey, new { @class = "form-control", @placeholder = "Enter Message Key", @aria_describedby = "msgHelp", @tabindex = 22 })
                        <small id="msgHelp" class="form-text text-muted d-none"></small>
                    </div>
                </div>
                <div class="form-group">
