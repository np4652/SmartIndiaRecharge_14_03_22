                    @Html.TextBox("txtSuccess", _Model.SuccessCode, new { @class = "form-control", @placeholder = "Enter Codes", @aria_describedby = "errSucHelp", @tabindex = 23 })
                    <small id="errSucHelp" class="form-text text-muted">Hint : Comma(,) seprated codes eg:-SUCCESS,successfull,2</small>
                </div>
                <div class="form-group">
                    @Html.Label("txtFail", "Failed Codes", new { @class = "lbl" })
                    <div class="input-group">
