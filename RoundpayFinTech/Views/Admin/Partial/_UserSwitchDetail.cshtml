@model IEnumerable<RoundpayFinTech.AppCode.Model.Userswitch>

@{
    var couter = 1;
}
@if (Model.Any())
{
    foreach (var item in Model)
    {
        <tr data-item-id="@item.OID"  data-item-userid="@item.UserID">
            <td class="text-right">@couter</td>
            <td><button class="btn btn-outline-danger btn-sm btnUnassign" id="btnUnassign">Unassign</button></td>
            <td>@item.OutletName <span class="text-monospace">[@(item.MobileNo + "/" + item.Prefix + item.UserID)]</span></td>
            <td>@item.Role</td>
            <td>@item.Operator</td>
            <td>@item.APIName</td>
        </tr>
        couter = couter + 1;
    }
}
else
{
    <tr rowspan="2">
        <td colspan="12">
            <center>No Record Found</center>
        </td>
    </tr>
}


