<style>
    body {
        overflow-x: hidden;
    }

    #wrapper {
        padding-left: 0;
        -webkit-transition: all 0.5s ease;
        -moz-transition: all 0.5s ease;
        -o-transition: all 0.5s ease;
        transition: all 0.5s ease;
    }

        #wrapper.toggled {
            padding-left: 250px;
        }

    #sidebar-wrapper {
        position: absolute;
        left: 267px;
        width: 0;
        height: 92%;
        margin-left: -250px;
        overflow-y: auto;
        background: #ffffff;
        border: 1px solid#dad9d9;
        box-shadow: 2px -3px #d8d8d8;
    }

    #wrapper.toggled #sidebar-wrapper {
        width: 250px;
    }

    #page-content-wrapper {
        width: 100%;
        position: absolute;
        padding: 15px;
        height: 944px;
        overflow: auto;
    }

    #wrapper.toggled #page-content-wrapper {
        position: absolute;
        margin-right: 0px;
    }


    /* Sidebar Styles */

    .sidebar-nav {
        position: absolute;
        top: 0;
        width: 250px;
        margin: 0;
        padding: 0;
        list-style: none;
    }

        .sidebar-nav li {
            text-indent: 20px;
            line-height: 40px;
        }

            .sidebar-nav li a {
                display: block;
                text-decoration: none;
                color: #3c3b3b;
            }

                .sidebar-nav li a:hover {
                    text-decoration: none;
                    color: #2013e8;
                    background: rgba(255, 255, 255, 0.2);
                }

                .sidebar-nav li a:active, .sidebar-nav li a:focus {
                    text-decoration: none;
                }

        .sidebar-nav > .sidebar-brand {
            height: 65px;
            font-size: 18px;
            line-height: 60px;
        }

            .sidebar-nav > .sidebar-brand a {
                color: #999999;
            }

                .sidebar-nav > .sidebar-brand a:hover {
                    color: #fff;
                    background: none;
                }

    .cusnav {
        list-style: disc;
        color: #fff;
        padding-left: 0rem;
    }

        .cusnav li {
            font-size: 13px;
            line-height: 25px
        }

    @@media(min-width:768px) {
        #wrapper {
            padding-left: 0;
        }

            #wrapper.toggled {
                padding-left: 250px;
            }

        #sidebar-wrapper {
            width: 0;
        }

        #wrapper.toggled #sidebar-wrapper {
            width: 250px;
        }

        #page-content-wrapper {
            padding: 20px;
            position: relative;
        }

        #wrapper.toggled #page-content-wrapper {
            position: relative;
            margin-right: 0;
            background-color: darkgrey;
        }
    }

    @@media only screen and (max-width: 600px) {
        #wrapper.toggled #sidebar-wrapper {
            width: 100%;
        }

        #sidebar-wrapper {
            position: relative;
            left: 267px;
            width: 0;
            height: 4%;
            margin-left: -265px;
            overflow-y: hidden;
            background: #009688;
        }

        .sidebar-nav {
            position: relative;
        }

        #wrapper.toggled #page-content-wrapper {
            position: inherit;
            margin-right: 0px;
        }

        #page-content-wrapper {
            width: 100%;
            position: absolute;
            padding: 5px;
            height: auto;
            overflow: hidden;
        }

        .sidebar-nav {
            width: 100%;
        }

        #wrapper.toggled {
            padding-left: 0;
        }
    }

    .well {
        min-height: 20px;
        padding: 19px;
        margin-bottom: 20px;
        background-color: #f5f5f5;
        border: 1px solid #e3e3e3;
        border-radius: 4px;
        -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.05);
        box-shadow: inset 0 1px 1px rgba(0,0,0,.05);
    }
