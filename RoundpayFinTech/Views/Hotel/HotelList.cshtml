        function getHotelInfo() {

            let arrHotelCodes = [];
            $(".listContainer").find(".singleItem").each(function () {
                arrHotelCodes.push($(this).attr("data-hotelid"));
            })
            let cityId = $("#hdnCityId").val();
            let req = { TBOHotelCodes: arrHotelCodes, CityId: cityId };
            //Post method will be here for data fetch of hotel info

            $.post("/Get-Hotel-Info", req)
                .done(function (res) {
                    console.log(res)
                    $(".listContainer").find(".singleItem").each(function () {
                        let _thisSingleItem = $(this);
                        let _thisHotelCode = $(this).attr("data-hotelid");
                        res.forEach(function (item, index) {
                            let itemHotelCode = item["@@TBOHotelCode"];
                            let itemHotelName = item["@@HotelName"];
                            let itemHotelimage = item["@@ImageUrl"];
                            let itemFacility = item["@@Facility"];
                            if (itemHotelCode == _thisHotelCode) {
                                let lat = item.position["@@Latitude"],
                                    lon = item.position["@@Longitude"];
                                let mapURL = "https://www.google.com/maps/search/?api=1&query=" + lat + "," + lon;
                                _thisSingleItem.find(".map-open").attr("data-map-url", mapURL);
                                _thisSingleItem.find("span[itemprop='ratingCount']").attr("content", item.award["@@Rating"]).text(item.award["@@Rating"] > 1 ? item.award["@@Rating"] + " Ratings" : item.award["@@Rating"] + " Rating");
                                let addtext = _thisSingleItem.find("div[itemprop='address']").find(".addressText");
                                let hotelName = _thisSingleItem.find("div[itemprop='name']").find(".hotelname");
                                let Facility = _thisSingleItem.find("div[itemprop='Facility']").find(".facilities");
                                let nearby = _thisSingleItem.find("div[itemprop='nearby']").find(".hnearby");
                                let thnearby = _thisSingleItem.find("div[itemprop='nearby']").find(".thnearby");
                                
                                debugger;
                                let hotelimgtag = _thisSingleItem.find("div[itemprop='imghotel']").find(".imghotels");
                                if (nearby.text()?.trim() == "") {
                                    nearby.text(item.address.addressLine[0]);
                                }
                                if (thnearby.text()?.trim() == "") {
                                    thnearby.text(item.address.addressLine[1]);
                                }
                                if (addtext.text()?.trim() == "") {
                                    addtext.text(item.address.addressLine[0]);
                                }
                                if (hotelName.text()?.trim() == "") {
                                    hotelName.text(itemHotelName);
                                }
                                if (Facility.text()?.trim() == "") {
                                    Facility.text(itemFacility);
                                    _thisSingleItem.find("div[itemprop='dvmore']").show();
                                    _thisSingleItem.find("span[itemprop='facicon']").show();
                                }
                                if (hotelimgtag.attr('src').indexOf("hotelnotfound.jpg") > -1) {
                                    hotelimgtag.attr('src', itemHotelimage);
                                }
                                //let anemList = createAnemeties(item.attributes.attribute);
                                //_thisSingleItem.find(".anemeties").append(anemList);
                            }
                        })
                    })
                })
        }

