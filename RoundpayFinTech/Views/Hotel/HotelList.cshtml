                    var finalprice = _thisPrice == "" ? 0 : Number.parseInt(_thisPrice);
                    $.each(arrCheckedprice, function (index, value) {
                        var prices = value.split('-');
                        var pric1 = prices[0] == "" ? 0 : Number.parseInt(prices[0]);
                        var pric2 = prices[1] == "" ? 0 : Number.parseInt(prices[1]);
                        if (pric1 == 8000) {
                            // finalprice >= pric1 ? $(this).show() : $(this).hide();
                            valsh = finalprice >= pric1 ? 1 : 2;
                        }
                        else if (finalprice >= pric1 && finalprice <= pric2) {
